    for( i = 1; i <= n1 && i <= n2; ++i)  
    {  
        if (n1 % i ==0 && n2 % i == 0)  
            GCD_Num = i;  #include <stdio.h>

int main() {
    int n;
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);
    double arr[n];

    
    printf("Enter the elements of the array:\n");
    for (int i = 0; i < n; ++i) {
        printf("Element %d: ", i + 1);
        scanf("%lf", &arr[i]);
    }
    double largest = arr[0];
    for (int i = 1; i < n; ++i) {
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }
    printf("The largest element in the array is: %.2lf\n", largest);

    return 0;
}

    }
    printf (" GCD of two numbers %d and %d is %d.", n1, n2, GCD_Num);  
